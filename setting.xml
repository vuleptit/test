<?xml version="1.0" encoding="UTF-8" ?>
<!-- Time in seconds -->
<httppush resettime="3600">
<alert1>
    <repeat>3</repeat>
    <timeinterval>3</timeinterval>
    <disable>False</disable> <!--True or False-->
    <!-- have not use time to activate yet -->
    <alarmcoolingperiod interval="30" timetoactivate="90">True</alarmcoolingperiod>
</alert1>
<alert2>
    <repeat>3</repeat>
    <timeinterval>3</timeinterval>
    <disable>True</disable> <!--True or False-->
    <closemode>close</closemode>
</alert2>
<alert3>
    <repeat>3</repeat>
    <timeinterval>3</timeinterval>
    <disable>False</disable> <!--True or False-->
    <closemode>open</closemode>
</alert3>
<alert4>
    <repeat>3</repeat>
    <timeinterval>3</timeinterval>
    <disable>False</disable> <!--True or False-->
    <closemode>open</closemode>
</alert4>
<alert5>
    <repeat>3</repeat>
    <timeinterval>3</timeinterval>
    <disable>False</disable> <!--True or False-->
    <closemode>open</closemode>
</alert5>
<endpoint>localhost://trigger/free</endpoint>
</httppush>
